{
    "paths": {    
      "reports": "reports",
      "scripts": "scripts",
      "tests": "tests",
      "baseline": "baseline"
    },
    "configs": {
      "env": "dev",
      "baseUrl": "{{baseUrl}}",   
      "browserName": "firefox",
